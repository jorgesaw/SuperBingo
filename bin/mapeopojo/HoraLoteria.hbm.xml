<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class
        name="ar.com.jorgesaw.superbingo.modelo.dto.HoraLoteria"
        table="HORA_LOTERIAS" >

        <!-- <cache usage="read-write" /> -->

        <id
            name="idHora"
            column="id_hora"
            length="8"
            type="int" >

            <generator class="native" />
        </id>
		<property name="horario" column="horario" type="string" length="40"
             not-null="true" />

    </class>

</hibernate-mapping>